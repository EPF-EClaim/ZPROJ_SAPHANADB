COLUMN TABLE "ZKWSP_MILEAGE" (
  "FROM_STATE_ID" CHAR(3) UNIQUE NOT NULL COMMENT 'From State ID',
  "FROM_BRANCH_ID" CHAR(10) UNIQUE NOT NULL COMMENT 'From Branch ID',
  "TO_STATE_ID" CHAR(3) UNIQUE NOT NULL COMMENT 'To State ID'
  ,
  "TO_BRANCH_ID" CHAR(10) UNIQUE NOT NULL COMMENT 'To Branch ID',
  "MILEAGE" CHAR(15) COMMENT 'Mileage',
  "MAX_MILEAGE" CHAR(15) COMMENT 'Max Mileage',
  PRIMARY KEY ("FROM_STATE_ID","FROM_BRANCH_ID","TO_STATE_ID","TO_BRANCH_ID")
) COMMENT 'KWSP Mileage'