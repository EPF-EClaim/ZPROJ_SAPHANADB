column table "ZLOC_TYPE" (
    "LOC_TYPE_ID" CHAR(6) unique not null COMMENT 'Loc_Type_ID',
    "LOC_TYPE_DESC" CHAR(100) COMMENT 'Loc_Type_Desc',
    "STATE1" CHAR(10) COMMENT 'State1',
    "STATE2" CHAR(10) COMMENT 'State2',
    "FROM_LOCATION" CHAR(10) COMMENT 'From_Location',
    "TO_LOCATION" CHAR(10) COMMENT 'To_Location',
    PRIMARY KEY ("LOC_TYPE_ID")
) COMMENT 'Location Type'
