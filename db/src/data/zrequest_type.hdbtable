column table "ZREQUEST_TYPE" (
    "REQUEST_TYPE_ID" CHAR(3) unique not null COMMENT 'Request Type ID',
    "REQUEST_TYPE_DESC" CHAR(100) COMMENT 'Request Type Desc',
    "START_DATE" DATE COMMENT 'Start Date',
    "END_DATE" DATE COMMENT 'End Date',
    "STATUS" CHAR(1) COMMENT 'Status',
    PRIMARY KEY ("REQUEST_TYPE_ID")
) COMMENT 'Request Type'
